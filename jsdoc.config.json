{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 5,
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "includeDate": true,
      "outputSourceFiles": false
    }
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "destination": "./docs/"
  }
}
