{
  "name": "@qanary/spring-boot-health-check",
  "version": "0.0.3",
  "description": "A straightforward component to check the status of a Spring Boot service.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^18.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
